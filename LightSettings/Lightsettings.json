{
    "editor.fontSize": 16.9,
    "editor.fontFamily": "'Geist Mono', 'Monaspace Neon', monospace,'Droid Sans Mono', 'monospace', monospace",
    "editor.fontLigatures": true,
    "terminal.integrated.fontSize": 16,
    "editor.minimap.enabled": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/**.out": true,
        "**/**.class": true,
        "**/**.idea": true,
        "**/node_modules": true,
        "**/dist": true,
        "**/out": true,
        "**/**.iml": true,
        "**/**.exe": true,
    },
    "git.autofetch": true,
    "editor.wordWrap": "on",
