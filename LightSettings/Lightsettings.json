{
    "editor.fontFamily": "'IBM Plex Mono', JetBrainsMono Nerd Font, Consolas, 'Courier New', monospace",
    "editor.fontWeight": "430",
    "editor.fontLigatures": true,
    "workbench.activityBar.location": "top",
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.stickyScroll.enabled": false,
    "editor.lineHeight": 1.8,
    "explorer.decorations.badges": false,
    "editor.fontSize": 14.1,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 2,
    "editor.renderWhitespace": "none",
    "terminal.integrated.fontWeight": "bold",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.cursorStyleInactive": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.shellIntegration.decorationsEnabled": false,
    "terminal.integrated.smoothScrolling": true,
    "files.exclude": {
        "**/**.class": true,
        "**/**.exe": true,
        "**/**.out": true,
        ".cph" : true,
        ".vscode" : true,
        "**/**.bin" : true
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "java": true,
        "c": true
    },
    "workbench.editor.enablePreview": false,
    "workbench.iconTheme": "a-file-icon-vscode",
    "workbench.colorCustomizations" : {
        "editor.background": "#0b0e14",
        "editor.foreground": "#bfbdb6",
        "terminal.background": "#0b0e14",
        "terminal.foreground": "#bfbdb6",
        "sideBar.background": "#0b0e14",
    //     "statusBar.background": "#0b0e14",
    //     "statusBar.foreground": "#bfbdb6",
        "tab.activeBackground": "#0b0e14",
        "tab.activeForeground": "#bfbdb6",
        "tab.inactiveBackground": "#0b0e14",
        "tab.inactiveForeground": "#bfbdb6",
    //     "titleBar.activeBackground": "#0b0e14",
    //     "titleBar.activeForeground": "#bfbdb6",
    //     "titleBar.inactiveBackground": "#0b0e14",
    //     "titleBar.inactiveForeground": "#bfbdb6",
    //     "foreground": "#bfbdb6",
    },
    "editor.wordWrap": "on",
    "java.server.launchMode": "lightweight",
    "terminal.integrated.mouseWheelZoom": true,
